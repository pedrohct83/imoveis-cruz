<!-- Starting MongoDB -->
cd ~
sudo service mongod stop
./mongod


<!-- Starting the app -->
cd ~
cd environment/Imoveis
nodemon --no-deprecation


<!-- Starting node-sass watch -->
cd ~
cd environment/Imoveis/public
node-sass -o css scss -w


<!-- Starting browserify/watchify -->
cd ~
cd environment/Imoveis/public/js
/public/js watchify main.js -o bundle.js -v



